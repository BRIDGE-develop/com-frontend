{
    "name": "com-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "cross-env PORT=8080 react-app-rewired start start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test --coverage",
        "eject": "react-app-rewired eject",
        "lint": "eslint ./src/**/*.{ts,tsx} --fix"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "react": "^16.9.0",
        "react-app-rewire-hot-loader": "^2.0.1",
        "react-app-rewired": "^2.1.3",
        "react-dom": "^16.9.0",
        "react-hot-loader": "^4.12.11",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.87.3",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@hot-loader/react-dom": "^16.9.0",
        "@testing-library/jest-dom": "^4.0.0",
        "@testing-library/react": "^9.1.3",
        "@types/axios": "^0.14.0",
        "@types/jest": "24.0.15",
        "@types/node": "12.0.10",
        "@types/react": "16.8.22",
        "@types/react-dom": "16.8.4",
        "@types/react-router-dom": "^4.3.4",
        "@types/validator": "^10.11.2",
        "@types/webpack-env": "^1.14.0",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "axios-mock-adapter": "^1.17.0",
        "babel-jest": "^24.9.0",
        "cross-env": "^5.2.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-jest": "^22.14.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react-hooks": "^1.6.1",
        "husky": "^3.0.2",
        "lint-staged": "^9.2.1",
        "post-npm-install": "^1.0.0",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "ts-jest": "^24.0.2",
        "typescript": "3.5.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:3000",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --verbose && lint-staged && cross-env CI=true npm run test",
            "post-merge": "post-npm-install"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint"
    }
}
